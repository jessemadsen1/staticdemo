@page "/"

<PageTitle>Index</PageTitle>

<h1>Hello, world!</h1>
<button>@onclick=buttonClick>Click Me </button>
<p>You click @timesClicked, as recently as @lastClicked</p>

@code
{
    int timesClicked = 0;
    Datetime lastClicked;
    void buttonClick()
    {
        timesClicked++;
        lastClicked = Datetime.Now;
    }
}

